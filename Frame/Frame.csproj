        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UseWPF>true</UseWPF>
        <ApplicationIcon>img\cof.ico</ApplicationIcon>
    </PropertyGroup>

    <ItemGroup>
      <Compile Update="Pixel.cs">
